		<li>Leathery</li>
	</stuffCategories>
	<costStuffCount>45</costStuffCount>

	<equippedStatOffsets>
		<PawnBeauty>0.1</PawnBeauty>
	</equippedStatOffsets>

	<apparel>
		<developmentalStageFilter>Child</developmentalStageFilter>
		<bodyPartGroups>
			<li>Shoulders</li>
			<li>Torso</li>
			<li>Legs</li>
			<li>GenitalsBPG</li>
		</bodyPartGroups>
		<layers>
			<li>Middle</li>
		</layers>
		<wornGraphicPath>Things/Apparel/Child/MamaDress</wornGraphicPath>
		<useWornGraphicMask>true</useWornGraphicMask>
	</apparel>

	<colorGenerator Class="ColorGenerator_StandardApparel" />
